<h1>Google maps</h1>
<p:gmap center="14.7221304, -17.496043" zoom="15" type="hybrid"
style="width:600px; height:400px"></p:gmap>

<script>
	let map;
	
	async function initMap() {
		//@ts-ignore
		const { Map } = await google.maps.importLibrary(maps);
		
		map = new Map(document.getElementById("maps"), {
			center: { lat:14.7221304, lng: -17.496043 },
			zoom: 15,
		});
	}
	
	iniMap();
</script>

